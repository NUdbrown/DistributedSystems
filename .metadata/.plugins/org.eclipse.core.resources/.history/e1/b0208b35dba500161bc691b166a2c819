
public enum CrudOperations {
	ADD("1"),
	DEL("2"),
	UPDATE("3"),
	GET("4");
	
	private final CrudOperations text;

    /**
     * @param text
     */
    private CrudOperations(final CrudOperations text) {
        this.text = text;
    }

    @Override
    public String toString() {
        return text;
    }
}

